  "license": "MIT",
  "description": "",
  "main": "index.js",
  "keywords": [],
  "type": "module",
  "author": "cxe.io",
  "contributors": [
    "Ian Carter"
  ],
  "scripts": {
    "test": "bash -c 'NODE_OPTIONS=--experimental-vm-modules jest'"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/cxe/client"
  },
  "exports": {
    ".": {
      "import": "./index.js"
    },
    "./*": {
      "import": "./lib/*.js"
    }
  },
  "devDependencies": {
    "jest": "30.0.4"
  },
  "jest": {
      "roots": ["<rootDir>/tests"],
      "coverageDirectory": "tmp/coverage",
      "testMatch": [
          "**/tests/**/*.?(m)js?(x)",
          "**/?(*.)+(spec|test).?(m)js?(x)"
      ],
      "transformIgnorePatterns": ["/node_modules/"],
      "transform": {},
      "testEnvironment": "node",
      "verbose": false
  }
}
